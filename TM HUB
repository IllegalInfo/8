-- Scripts:

local function CWPBYI_fake_script() -- LOADTM.LocalScript 
	local script = Instance.new('LocalScript', LOADTM)

	local color1 = Color3.fromRGB(26, 3, 155)
	local color2 = Color3.fromRGB(146, 43, 255) 
	local changeSpeed = 10 
	spawn(function()
	          local i = -1
	          while true do
	              for i=-1,1,1/changeSpeed do
	                  local f = math.abs(i)
	                  script.Parent.TextColor3 = Color3.fromRGB(
	                      255 * (color1.r + (color2.r - color1.r) * f),
	                      255 * (color1.g + (color2.g - color1.g) * f), 
	                      255 * (color1.b + (color2.b - color1.b) * f))
	                  wait(0.05)                          
	              end
	          end
	      end) 
end
coroutine.wrap(CWPBYI_fake_script)()
local function DTCT_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		HubPURGE:Destroy()
	end)
	
	local color1 = Color3.fromRGB(26, 3, 155)
	local color2 = Color3.fromRGB(146, 43, 255) 
	local changeSpeed = 10 
	spawn(function()
	          local i = -1
	          while true do
	              for i=-1,1,1/changeSpeed do
	                  local f = math.abs(i)
	                  script.Parent.BorderColor3 = Color3.fromRGB(
	                      255 * (color1.r + (color2.r - color1.r) * f),
	                      255 * (color1.g + (color2.g - color1.g) * f), 
	                      255 * (color1.b + (color2.b - color1.b) * f))
	                  wait(0.05)                          
	              end
	          end
	      end) 
end
coroutine.wrap(DTCT_fake_script)()
local function VSGHJC_fake_script() -- Continue.LocalScript 
	local script = Instance.new('LocalScript', Continue)

	if game.PlaceId == 177052655 then
		script.Parent.MouseButton1Click:Connect(function()
			loadstring(game:HttpGet'https://raw.githubusercontent.com/IllegalInfo/8/master/ZANEXXV2')()
			HubPURGE:Destroy()
			end)
	else
	    return
	end
	
	local color1 = Color3.fromRGB(26, 3, 155)
	local color2 = Color3.fromRGB(146, 43, 255) 
	local changeSpeed = 10 
	spawn(function()
	          local i = -1
	          while true do
	              for i=-1,1,1/changeSpeed do
	                  local f = math.abs(i)
	                  script.Parent.BorderColor3 = Color3.fromRGB(
	                      255 * (color1.r + (color2.r - color1.r) * f),
	                      255 * (color1.g + (color2.g - color1.g) * f), 
	                      255 * (color1.b + (color2.b - color1.b) * f))
	                  wait(0.05)                          
	              end
	          end
	      end) 
	
end
coroutine.wrap(VSGHJC_fake_script)()
