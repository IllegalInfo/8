end)


addcmd(
    "radio",
    "plays an audio",
    {},
    function(args)
        local players = getPlayer(args[1])
        if not args[1] or not args[2] then
            return
        end
        local num = args[2]
        for i, v in pairs(players) do
            game.ReplicatedStorage.Event:FireServer("PlayRadio", gPlayers[v], num)
        end
    
    MessageColor("[$incity LCM]: Now Playing The Audio On "..Players.."", Color3.fromRGB(252, 101, 220))

end)
