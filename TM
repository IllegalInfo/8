local function MFNVSU_fake_script() -- TextBox.LocalScript
local script = Instance.new('LocalScript', TextBox)

local TextBox = script.Parent
if TextBox.FocusLost:Connect(function()
game.ReplicatedStorage.Event:FireServer("PlayRadio", game.Players.LocalPlayer, TextBox.Text)
end)
then end
end
coroutine.wrap(MFNVSU_fake_script)()
