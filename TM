Sincity("lk", "Loopkills The Chosen Player", function(args)
  local players = FindPlr(args[1])
  for i, v in pairs(players) do
    if FP[v]:FindFirstChild("ml") then
        FP[v]:FindFirstChild("ml"):Destroy()
    end
    local ml = Instance.new("BoolValue")
    ml.Parent = FP[v]
    ml.Value = true
    ml.Name = "ml"
    while FP[v]:FindFirstChild("ml") do
    if FP[v]:FindFirstChild("ml") then
  wait(.01)
      local plrs = game:GetService("Players"):FindFirstChild(FP[v].Name).Character:FindFirstChild("Humanoid")
      game.ReplicatedStorage.Event:FireServer("TPD", math.huge, plrs)
  end
  end
  end
  SendM("[$incity LCM]: Player(-s) Loopkilled", Color3.fromRGB(87, 0, 138))
  end)


Sincity("unlk", "Unloopkills The Chosen Player", function(args)
local players = FindPlr(args[1])
for i, v in pairs(players) do
  if FP[v]:FindFirstChild("ml") then
    FP[v]:FindFirstChild("ml"):Destroy()
  end
end
SendM("[$incity LCM]: Player(-s) Unloopkilled", Color3.fromRGB(87, 0, 138))
end)



Sincity("g", "Gods The Chosen Player", function(args)
local players = FindPlr(args[1])
RA()
for i, h in pairs(players) do
game.ReplicatedStorage.Event:FireServer("TPD", (-0.53220192342997) ^ 1.45, FP[h].Character.Humanoid)
end
SendM("[$incity LCM]: Player(-s) Godded", Color3.fromRGB(87, 0, 138))
end)


Sincity("lg", "Loopgods The Chosen Player", function(args)
local players = FindPlr(args[1])
RA()
for i, v in pairs(players) do
if FP[v]:FindFirstChild("god") then
FP[v]:FindFirstChild("god"):Destroy()
end
local god = Instance.new("BoolValue")
god.Parent = FP[v]
god.Value = true
god.Name = "god"
game:GetService("RunService").RenderStepped:connect(function()
  game.ReplicatedStorage.Event:FireServer("TPD", math.huge / 1.45, FP[v].Character.Humanoid)
  game.ReplicatedStorage.Event:FireServer("TPD", -math.huge ^ 1.45, FP[v].Character.Humanoid)
end)
end
SendM("[$incity LCM]: Player(-s) Loopgodded", Color3.fromRGB(87, 0, 138))
end)
  

Sincity("unlg", "Unloopgods The Chosen Player", function(args)
local players = FindPlr(args[1])
for i, v in pairs(players) do
if FP[v]:FindFirstChild("god") then
FP[v]:FindFirstChild("god"):Destroy()
end
end
SendM("[$incity LCM]: Player(-s) Unloopgodded", Color3.fromRGB(87, 0, 138))
end)



Sincity("poison", "Poisons The Chosen Player", function(args)
local players = FindPlr(args[1])
for i, p in pairs(players) do
while wait(.1) do
  game.ReplicatedStorage.Event:FireServer("TPD", 1, FP[p].Character.Humanoid)
  if FP[p].Character.Humanoid.Health == 0 then
    break
end
end
end
end)



