Sincity("nc", "Enables Noclip For The LocalPlayer Only", function(args)
local noclip = true 
char = game.Players.LocalPlayer.Character
while true do
if noclip == true then 
for _,v in pairs(char:children()) do 
pcall(function() 
if v.className == "Part" then v.CanCollide = false
elseif v.ClassName == "Model" then 
v.Head.CanCollide = false
end 
end)
end 
end 
game:service("RunService").Stepped:wait() 
end
popup("Noclip Enabled")
end)



Sincity("whitel", "Lets Other Players Use Your Commands", function(args)
    if not args[1] then
        return
    end
    local players = FindPlr(args[1])
    table.foreach(players, function(k, v)
      admins[v] = true
        end)
end)



Sincity("unwhitel", "Disables The Chosen Players Whitelist", function(args)
  if not args[1] then
    return
end
    local players = FindPlr(args[1])
    table.foreach(players, function(k, v)
      admins[v] = nil
        end)
end)



Sincity("cl", "Disables Noclip", function(args)
local noclip = false
char = game.Players.LocalPlayer.Character
while true do
if noclip == false then 
for _,v in pairs(char:children()) do 
pcall(function() 
if v.className == "Part" then v.CanCollide = true
elseif v.ClassName == "Model" then 
v.Head.CanCollide = true
end 
end)
end 
end 
game:service("RunService").Stepped:wait()
end
popup("Noclip Disabled") 
end) 



Sincity("esp", "Enables A Custom ESP For The Chosen Player", function(args)
local players = FindPlr(args[1])
for i, v in pairs(players) do
for b,n in pairs(FP[v].Character:GetChildren()) do
for i,m in pairs(FP[v].Character:GetChildren()) do
if m:IsA("Part") or m:IsA("MeshPart") then
if m.Name ~= "HumanoidRootPart" and m.Name ~= "Handle" or (m:IsA("Part")) then
m.Material = "ForceField"
m.BrickColor = BrickColor.new("Baby Blue")
end
end
end
end
if game.Players[FP[v]].Name == "exclusiveclique" then 
m.BrickColor = BrickColor.new("Really Black")
end
end
end)



