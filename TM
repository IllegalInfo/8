    function(args)
        local players = getPlayer(args[1])
        for i, v in pairs(players) do
            local workspacep = game.Workspace.Ignore.Players
            local audio = workspacep:FindFirstChild(players[1])
            game.ReplicatedStorage.Event:FireServer("Cloak", audio)
        end
    
    MessageColor("[$incity LCM]: "..Players.." Cloaked", Color3.fromRGB(252, 101, 220))
end)
addcmd(
    "Ws",
    "changes the localplayers walkspeed",
    {},
    function(args)
        local walkspeedvalue = args[1]
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = walkspeedvalue
        MessageColor("[$incity LCM]: Walkspeed Changed To ["..walkspeedvalue.."]", Color3.fromRGB(252, 101, 220))
    end)
    addcmd(
        "Prefix",
        "Lets you change the prefix to anything",
        {},
        function(args)
            wait()
            prfx = args[1]
        
        MessageColor("[$incity LCM]: Prefix Changed To ["..prfx.."]", Color3.fromRGB(252, 101, 220))

end)

addcmd(
    "bg",
    "brings the gun to you",
    {},
    function(args)
        game.Workspace.Gun.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
wait(1)
        game.Workspace.Gun.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
wait(1)
        game.Workspace.Gun.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        MessageColor("[$incity LCM]: Gun Teleported!", Color3.fromRGB(252, 101, 220))
    end)

addcmd(
    "fly",
    "makes the localplayer fly",
{},
    function(args)
        MessageColor("[$incity LCM]: You Are Now Flying! Press E To Stop", Color3.fromRGB(252, 101, 220))
loadstring(game:HttpGet(('https://pastebin.com/raw/6KVugL85'),true))()

    end)


addcmd(
    "rejoin",
    "makes the localplayer rejoin",
    {},
    function(args)
        MessageColor("[$incity LCM]: Rejoining.. Please Wait!", Color3.fromRGB(252, 101, 220))
        game:GetService("TeleportService"):Teleport(game.PlaceId)
    end
)

addcmd(
    "leave",
    "shutdowns the localplayers game",
    {},
    function(args)
        MessageColor("[$incity LCM]: Rage Quit lol", Color3.fromRGB(252, 101, 220))
        wait(.5)
        game:Shutdown()
    end
)

addcmd(
    "reset",
    "Makes you reset.",
    {},
    function(args)
        MessageColor("[$incity LCM]: Holl up lemme get my shotgun Xddddd", Color3.fromRGB(252, 101, 220))
        game.Players.LocalPlayer.Character:BreakJoints()
    end
)

addcmd(
    "shiftlock",
    "enables shiftlock",
    {},
    function(args)
        game.Players.LocalPlayer.DevEnableMouseLock = true
        MessageColor("[$incity LCM]: Shiftlock Enabled", Color3.fromRGB(252, 101, 220))
    end)

local Player = game.Players.LocalPlayer
Player.Chatted:connect(
    function(cht)
        if cht:match(prfx .. "noclip") then
            local function noclip(plr)
                local admintag = Instance.new("BoolValue")
                admintag.Parent = game.Players[plr]
                admintag.Value = true
                admintag.Name = "noclip"
            end
            if game.Players.LocalPlayer:FindFirstChild("noclip") then
                game.Players.LocalPlayer.noclip:Destroy()
            end
            noclip(game.Players.LocalPlayer.Name)
            game:GetService("RunService").Stepped:connect(
                function()
                    if game.Players.LocalPlayer:FindFirstChild("noclip") then
                        game.Players.LocalPlayer.Character.Head.CanCollide = false
                        game.Players.LocalPlayer.Character.Torso.CanCollide = false
                    end
                end
            )
        end
    end
)

Player.Chatted:connect(
    function(cht)
        if cht:match(prfx .. "clip") then
            game.Players.LocalPlayer.noclip:Destroy()
        end
    end
)

local lighting = game.Lighting
if lighting:FindFirstChild("ColorCorrection") then
    lighting:FindFirstChild("ColorCorrection"):Remove()
end
if lighting:FindFirstChild("Correction") then
    lighting:FindFirstChild("Correction"):Remove()
end
if lighting:FindFirstChildOfClass("SunRaysEffect") then
    lighting:FindFirstChildOfClass("SunRaysEffect"):Remove()
end

local sunray = Instance.new("SunRaysEffect", lighting)
local sky = lighting.keasjdajshdahwieawd
sky.SkyboxBk = "http://www.roblox.com/asset/?id=149397692"
