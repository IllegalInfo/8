x.Position = x.Position + UDim2.new(0, 0, 0, -15)
end
  b.Parent = c
  b.Text = v.." "
  b.TextColor3 = w
  wait(15) for y = 0, 1, 0.1 do 
  wait(0.1)
  b.TextStrokeTransparency = 1
  b.TextTransparency = y 
  end
  b:Destroy()
  end)
  end


