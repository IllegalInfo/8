local function PAYLM_fake_script() -- Ping.LocalScript
local script = Instance.new('LocalScript', Ping)

function GetPing()
local Ping = 300-((1/wait())*10)
if Ping < 1 then
Ping = 1
end
return math.floor(Ping)
end

while wait(1)  do
script.Parent.Text = "Ping > "..GetPing()
end
end
