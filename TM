function start()
  game:GetService('RunService').Stepped:connect(function()
  local n = game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("GUI") if n then
  for i, x in pairs(n.Chats:GetChildren()) do
    if x.Name == "Chat" then 
    
    if string.find(x.Text, "exclusiveclique") then
    x.Text = string.gsub(x.Text, "exclusiveclique", "[Owner] Zack") 
    x.TextColor3 = Color3.fromRGB(0, 0, 0)
     while true do
      for a = 1,360,1 do wait(.01)
       x.TextStrokeColor3 = Color3.fromHSV(a/360,1,1)
      end
     end

    
    elseif string.find(x.Text, "Jay_Trvp") then 
    x.Text = string.gsub(x.Text, "Jay_Trvp", "[crodie] Jay") 
    x.TextColor3 = Color3.fromRGB(0, 220, 0)
    
    
    elseif string.find(x.Text, "qtiepies") then 
    x.Text = string.gsub(x.Text, "qtiepies", "[Angel] Emmie") 
    x.TextColor3 = Color3.fromRGB(215, 148, 255)
end
end
end
end
end)
end

