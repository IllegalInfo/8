  player.Character.HumanoidRootPart.CFrame = CFrame.new(54.5063286, 22.1997204, -67.2636566, -0.999139071, 0, -0.041487325, 0, 1, 0, 0.041487325, 0, -0.999139071)
  mouse1click()
  mouse1click()
  mouse1click()
  wait()
  player.Character.HumanoidRootPart.CFrame = CFrame.new(54.1307602, 22.1997204, -86.355484, 0.0665355325, -1.61819109e-08, -0.997784078, 1.01435901e-07, 1, -9.45376755e-09, 0.997784078, -1.00582113e-07, 0.0665355325)
  mouse1click()
  mouse1click()
  mouse1click()
  wait()
  player.Character.HumanoidRootPart.CFrame = CFrame.new(28.9910889, 22.1997204, -86.3085785, 0.697867095, -3.1538633e-08, 0.716227233, 2.90973254e-08, 1, 1.56829589e-08, -0.716227233, 9.89567539e-09, 0.697867095)
  mouse1click()
  mouse1click()
  mouse1click()
  wait()
