local function QKINLLC_fake_script() -- FPS.LocalScript
local script = Instance.new('LocalScript', FPS)

local textLabel = script.Parent
local Delay = false
local Last = tick()

game:GetService("RunService").RenderStepped:Connect(function()
local FramePerSec = math.floor(.5+(1/(tick() - Last )))
if Delay == false then
Delay = true
textLabel.Text = "FPS > "..FramePerSec
Delay = false
end
Last = tick()
end)
end
coroutine.wrap(QKINLLC_fake_script)()
