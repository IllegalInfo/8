prfx = pargs
SendM("[$incity LCM]: Prefix Changed To: "..prfx.."", Color3.fromRGB(87, 0, 138))

if prfx == "!" then
popup("Prefix Set To [!] (Matts Admin)")

elseif prfx == "." then
popup("Prefix Set To [.] (Faxng Admin)")

elseif prfx == ":" then
popup("Prefix Set To [:] (Aidez)")

elseif prfx == "/" then
popup("Prefix Set To [/] (Terminal X)")

elseif prfx == "[]" then
popup("Prefix Set To [[]] ($incity)")
end
end)



