local counter = 0
local num = 10
if not _G.colorSequences then
	_G.colorSequences = {}
	while true do
		local sequence = {}
		for i = 0, num do
			table.insert(
				sequence, i + 1, ColorSequenceKeypoint.new(i / num, Color3.fromRGB(
					127 * math.sin(0.52 * i + counter) + 128,
					127 * math.sin(0.52 * i + 2 * math.pi / 3 + counter) + 128,
					127 * math.sin(0.52 * i + 4 * math.pi / 3 + counter) + 128)
				)
			)
		end
		local new = ColorSequence.new(sequence)
		if #_G.colorSequences > 0 then
			if new == _G.colorSequences[1] then
				break
			end
		end
		table.insert(_G.colorSequences, new)
		counter = counter + 0.0785
		if (counter >= 6.28) then
			counter = 0
		end
	end
end

if not _G.RGBDev then
	_G.RGBDev = {Lettering = {}, Connections = {}}
end

function RGBDev(name)
	if _G.RGBDev.Connections[name] then return end
	local plm = game:GetService("CoreGui").RobloxGui:FindFirstChild("PlayerListMaster")
	if not plm
	or not plm:FindFirstChild("OffsetFrame")
	or not plm.OffsetFrame:FindFirstChild("PlayerScrollList")
	or not plm.OffsetFrame.PlayerScrollList:FindFirstChild("SizeOffsetFrame")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame:FindFirstChild("ScrollingFrameContainer")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer:FindFirstChild("ScrollingFrameClippingFrame")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame:FindFirstChild("ScollingFrame")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScollingFrame:FindFirstChild("OffsetUndoFrame")
	then return else plm = plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScollingFrame.OffsetUndoFrame end
	_G.RGBDev.Connections[name] = game:GetService("RunService").RenderStepped:Connect(function()
		for _,v in pairs(plm:GetDescendants()) do
			if v:IsA("TextLabel") and v.Name == "PlayerName" and v.Text == name then
				if not _G.RGBDev.Lettering[v] then
					_G.RGBDev.Lettering[v] = Instance.new("UIGradient", v)
					v.TextColor3 = Color3.fromRGB(255, 255, 255)
				elseif math.ceil(tick()) % 0.5 == 0 then
					_G.RGBDev.Lettering[v].Color = _G.colorSequences[math.ceil((math.fmod(tick(), 1))*80)]
				end
				v.Rotation = math.random(-1,1)
				if Devs[name] then
					v.Parent.Parent.PlayerIcon.Image = "rbxasset://textures/ui/icon_admin-16.png"
				end
			end
		end
	end)
end

function colorName(name)
	local plm = game:GetService("CoreGui").RobloxGui:FindFirstChild("PlayerListMaster")
	if not plm
	or not plm:FindFirstChild("OffsetFrame")
	or not plm.OffsetFrame:FindFirstChild("PlayerScrollList")
	or not plm.OffsetFrame.PlayerScrollList:FindFirstChild("SizeOffsetFrame")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame:FindFirstChild("ScrollingFrameContainer")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer:FindFirstChild("ScrollingFrameClippingFrame")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame:FindFirstChild("ScollingFrame")
	or not plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScollingFrame:FindFirstChild("OffsetUndoFrame")
	then return else plm = plm.OffsetFrame.PlayerScrollList.SizeOffsetFrame.ScrollingFrameContainer.ScrollingFrameClippingFrame.ScollingFrame.OffsetUndoFrame end
	for i,v in pairs(plm:GetDescendants()) do
		if v:IsA("TextLabel") and v.Name == "PlayerName" and v.Text == name then
			if Donors[name] then
				if Tier[Donors[name]].Color == "RGBDev" then
					RGBDev(name)
				else
					v.TextColor3 = Tier[Donors[name]].Color
				end
			end
		end
	end
end



cmdp.PlayerAdded:Connect(function(v)
	if Devs[v.Name] then
		print("")
		RGBDev(v.Name)
	elseif Donors[v.Name] then
		print("")
		colorName(v.Name)
	end
end)

function note(msg,dismiss,decay,titleOverride)
	local msg = msg or "nigga"
	local dismiss = dismiss or false
	local decay = decay or 3
	local titleOverride = titleOverride or "Notification"
	local elements = {}
	local sg = Instance.new("ScreenGui",coregui)
	sg.Name = "Temp"
	local title = Instance.new("TextLabel",sg)
	title.Size = UDim2.new(0,400,0,40)
	title.Position = UDim2.new(0.5,-title.Size.X.Offset/2,0,150)
	title.Text = titleOverride
	title.Font = "SourceSansBold"
	title.BackgroundColor3 = colorScheme.Main
	title.TextColor3 = colorScheme.Text
	title.BackgroundTransparency = 0.2
	title.TextSize = title.Size.Y.Offset-10
	local notification = Instance.new("TextLabel",title)
	notification.Size = UDim2.new(0,title.Size.X.Offset,0,150)
	notification.Position = UDim2.new(0,0,1,0)
	notification.Text = msg
	notification.Font = "SourceSansItalic"
	notification.BackgroundColor3 = colorScheme.Main
	notification.TextColor3 = colorScheme.Text
	notification.BackgroundTransparency = 0.2
	notification.TextSize = 15
	notification.TextYAlignment = 0
	local image = Instance.new("ImageLabel",title)
	image.Image = "rbxassetid://1839007066"
	image.Size = UDim2.new(0,title.Size.Y.Offset-6,0,title.Size.Y.Offset-6)
	image.Position = UDim2.new(0,3,0,3)
	image.BackgroundTransparency = 1
	spawn(function()
		if not dismiss then
			wait(decay)
			sg:Destroy()
		else
			title.Active = true
			title.Draggable = true
			wait(2)
			local X = Instance.new("ImageButton",title)
			X.Image = "rbxassetid://1842003618"
			X.ImageColor3 = Color3.new(1,0,0)
			X.Size = UDim2.new(0,title.Size.Y.Offset-16,0,title.Size.Y.Offset-16)
			X.Position = UDim2.new(1,-X.Size.X.Offset-8,0,8)
			X.BackgroundTransparency = 1
			X.MouseButton1Up:connect(function()
				sg:Destroy()
			end)
		end
	end)
end
local popups = {}
function popup(msg,decay,titleOverride)
	local msg = msg or "nigga"
	local decay = decay or 3
	local titleOverride = titleOverride or "Notification"
	local offset = 0
	local spacing = 5
	if #popups > 0 then
		for i,v in next, popups do
			offset = offset + 50 + spacing
		end
	end
	local sg = Instance.new("ScreenGui",coregui)
	table.insert(popups,sg)
	local title = Instance.new("TextLabel",sg)
	title.Size = UDim2.new(0,200,0,20)
	title.Position = UDim2.new(1,0,1,-title.Size.Y.Offset-offset-30-spacing)
	title.Text = titleOverride
	title.Font = "SourceSansBold"
	title.BackgroundColor3 = colorScheme.Main
	title.TextColor3 = colorScheme.Text
	title.BackgroundTransparency = 0.2
	title.TextSize = title.Size.Y.Offset-5
	title.Name = "Title"
	local notification = Instance.new("TextLabel",title)
	notification.Size = UDim2.new(0,title.Size.X.Offset,0,30)
	notification.Position = UDim2.new(0,0,1,0)
	notification.Text = msg
	notification.Font = "SourceSansItalic"
	notification.BackgroundColor3 = colorScheme.Main
	notification.TextColor3 = colorScheme.Text
	notification.BackgroundTransparency = 0.2
	notification.TextSize = 15
	notification.TextWrapped = true
	notification.TextYAlignment = 0
	local image = Instance.new("ImageLabel",title)
	image.Image = "rbxassetid://1841191919"
	image.ImageColor3 = Color3.fromRGB(150,0,0)
	image.Size = UDim2.new(0,title.Size.Y.Offset-6,0,title.Size.Y.Offset-6)
	image.Position = UDim2.new(0,3,0,3)
	image.BackgroundTransparency = 1
	title:TweenPosition(UDim2.new(1,-title.Size.X.Offset,1,-title.Size.Y.Offset-offset-30-spacing))
	spawn(function()
		wait(decay)
		fade(title)
		for i,v in next, title:GetChildren() do
			fade(v)
		end
		repeat wait() until not title or title.BackgroundTransparency >= 1
		if sg then
			for i,v in next, popups do
				if sg == v then
					table.remove(popups,i)
				end
			end
			sg:Destroy()
			for i,v in next, popups do
				local title = v:FindFirstChild'Title'
				if title then
					title.Position = title.Position + UDim2.new(0,0,0,50+spacing)
				end
			end
		end
	end)
end
function fade(object)
	spawn(function()
		if object:IsA'TextLabel' or object:IsA'TextBox' then
			spawn(function()
				for i = object.TextStrokeTransparency,1.1,(1.1-object.TextStrokeTransparency)/50 do
					object.TextStrokeTransparency = i
					game:GetService'RunService'.RenderStepped:wait()
				end
			end)
			spawn(function()
				for i = object.TextTransparency,1.1,(1.1-object.TextTransparency)/50 do
					object.TextTransparency = i
					game:GetService'RunService'.RenderStepped:wait()
				end
			end)
		elseif object:IsA'ImageLabel' then
			for i = object.ImageTransparency,1.1,(1.1-object.ImageTransparency)/50 do
				object.ImageTransparency = i
				game:GetService'RunService'.RenderStepped:wait()
			end
		end
	end)
	spawn(function()
		for i = object.BackgroundTransparency,1.1,(1.1-object.BackgroundTransparency)/50 do
			object.BackgroundTransparency = i
			game:GetService'RunService'.RenderStepped:wait()
		end
	end)
	for i,v in next, object:GetDescendants() do
		fade(v)
	end
end

